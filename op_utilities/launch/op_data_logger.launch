<!-- -->
<launch>
	<arg name="mapSource" 				default="0" /> <!-- Autoware=0, Vector Map Folder=1, kml file=2, lanelet2 file=3 -->
	<arg name="mapFileName" 			default="/media/hatem/8ac0c5d5-8793-4b98-8728-55f8d67ec0f4/data/ToyotaCity2/map/vector_map/" />
	<arg name="lanelet2_origin"			default="0,0,0" /> <!-- GPS origin for lanelet2 map files, Lat, Long, Alt  -->
	
	<arg name="experimentName" 			default="" />	
	<arg name="lightsAndSignsLog" 		default="true" />
	<arg name="trackingLog"				default="false" />
	<arg name="predictionLog"			default="false" />
	<arg name="controlLog"				default="false" />
		
		
	<node pkg="op_utilities" type="op_data_logger" name="op_data_logger" output="screen">
		
		<param name="mapSource" 		value="$(arg mapSource)" />
		<param name="mapFileName" 		value="$(arg mapFileName)" />		
		<param name="lanelet2_origin" 	value="$(arg lanelet2_origin)" />	
		
		<param name="experimentName" 		value="$(arg experimentName)" />
		<param name="lightsAndSignsLog" value="$(arg lightsAndSignsLog)" />
		<param name="trackingLog" 		value="$(arg trackingLog)" />
		<param name="predictionLog" 	value="$(arg predictionLog)" />
		<param name="controlLog" 		value="$(arg controlLog)" />
	</node>

</launch>
